<<
~
>>
? delobj SEPPARAMDB -form blank 
<<
~
>>
? fill top902.osq 
!!
initialize (ans = varchar(80),
            num = integer2) = {}

'Go' = { ans := prompt 'Enter any string : ' with style = popup;
         message 'Finding its length...';
         sleep 2;
         num = callproc strln(ans);
         message 'The length of the string is ' + char(num);	
         sleep 2;
         message 'Changing it to lowercase...';
         sleep 2;
         callproc lowercase(byref(ans));
         message 'The following is the string in lowercase :';
         sleep 2;
         message :ans;
         sleep 2;
       }

'Quit' = { exit; }
!!
? fill strln.c 
!!
int
strln(s)
char *s;
{
   char *p = s;
   while (*p != '\0')
      p++;
   return (p-s);
}
!!
? fill lowercase.c 
!!
lowercase (p)
char *p;
{
   for (; *p != '\0'; ++p)
      if ('A' <= *p && *p <= 'Z')
         *p -= 'a';
}
!!
