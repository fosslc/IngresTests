!!
1    1.1       eleven
22   22.22     twenty-two
333  333.333   three hundred thirty-three
4444 4444.4444 four thousand four hundred forty-four
!!
? fill b2902.sql 
!!
copy table b2902 (
   intcol = c5,
   flt  = c10,
   str  = c0nl)
from 'b2902.d';
\p\g
!!
? fill s2902.frm 
!!
COPYFORM:	6.4	1990_08_09 00:13:53 GMT  
FORM:	s2902		
	80	23	0	0	0	0	0	8	0	0	0	0	0	0	0	0
FIELD:
TRIM:
!!
? fill s2902.osq 
!!
initialize (
   x	=	integer not null,
   y	=	float,
   z	=	char(40),
   fourteen =	integer) = 
   {
   message 'Select Values to view passed parameter values';
   sleep 1;
   }

'Values' = 
   {
   message 'X := ' + varchar(x) with style = popup;
   message 'Y := ' + varchar(y) with style = popup;
   message 'Z := ' + varchar(z) with style = popup;
   message 'fourteen := ' + varchar(fourteen) with style = popup;
   }

'Change' = 
   {
   x := 141414;
   }

'End' =
   {
   return;
   }
!!
