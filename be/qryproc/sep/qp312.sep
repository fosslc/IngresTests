        First fill files containing ESQL program
*/
? fill qp312_seqs.sc 
!!
/* This program will create the required sequences:
** One integer type named qp312_intseq with a MAXVAL and NOCYCLE set.
** One decimal type named qp312_decseq with a START, MINVALUE, and CACHE set.
**
*/

#include <stddef.h>
#include <stdio.h>
#include <stdlib.h>
EXEC SQL INCLUDE SQLCA;

int main(int argc, char *argv[])
{
   EXEC SQL BEGIN DECLARE SECTION;
      int i;
      char *dbname;
      int v1;
      int v2;
   EXEC SQL END DECLARE SECTION;

   dbname = argv[1];
   EXEC SQL WHENEVER SQLERROR stop;
   EXEC SQL CONNECT :dbname;

   EXEC SQL WHENEVER SQLERROR continue;

   printf("Dropping sequences...\n");
   EXEC SQL DROP SEQUENCE qp312_intseq;
   EXEC SQL DROP SEQUENCE qp312_decseq;

   EXEC SQL WHENEVER SQLERROR call sqlprint;

   printf("Creating sequences...\n");

   EXEC SQL CREATE SEQUENCE qp312_intseq
        maxvalue 1000000 no cache no cycle no order;
   printf("First sequence created...\n");

   EXEC SQL CREATE SEQUENCE qp312_decseq as decimal
        minvalue 1000 start with 1000 cache 100;
   printf("Second sequence created...\n");

   printf("Now doing select with NEXT VALUE/CURRENT VALUE...\n");
   printf("|---------|\n");
   printf("|NVAL|CVAL|\n");

   for (i=0; i<5; i++)
   {
      EXEC SQL SELECT 
         next value for qp312_intseq, 
         current value for qp312_intseq
         INTO :v1, :v2;
         printf("|----|----|\n");
         printf("|%4d|%4d|\n", v1, v2);
   }
   printf("|---------|\n");

   printf("Operation completed\n");

   EXEC SQL DISCONNECT;
}
!!
? fill qp312_tbls.sc 
!!
/* This program does the following:
**
** This program will create the required tables:
** First table will get created and populated from an AS SELECT of relid
** along with CURRENT VALUE and NEXT VALUE from the decimal sequence 
** qp312_decseq.
** Second table will be created with data types of int and char and will
** get populated from INSERT statements using NEXT VALUE and CURRENT VALUE 
** from the integer sequence qp312_intseq along with a character string.
**
** This program will also verify that NEXT VALUE can be used in an UPDATE
** statement.
**
*/

#include <stddef.h>
#include <stdio.h>
#include <stdlib.h>
EXEC SQL INCLUDE SQLCA;

int main(int argc, char *argv[])
{
   EXEC SQL BEGIN DECLARE SECTION;
      char *dbname;
      double tbl1c1;
      double tbl1c2;
      int tbl2c1;
      char tbl2c2[5];
      int tbl3c1;
      char tbl3c2[5];
   EXEC SQL END DECLARE SECTION;

   dbname = argv[1];
   EXEC SQL WHENEVER SQLERROR stop;
   EXEC SQL CONNECT :dbname;

   EXEC SQL WHENEVER SQLERROR continue;

   printf("Dropping tables...\n");
   EXEC SQL DROP TABLE qp312_tbl1;
   EXEC SQL DROP TABLE qp312_tbl2;
   EXEC SQL DROP TABLE qp312_tbl3;

   EXEC SQL WHENEVER SQLERROR call sqlprint;

   printf("Creating tables...\n");

   EXEC SQL CREATE TABLE qp312_tbl1 as select
        first 100 relid,
        current value for qp312_decseq as currcol,
        next value for qp312_decseq as nextcol
        from iirelation;
   printf("First table created...\n");
   EXEC SQL CREATE TABLE qp312_tbl2(
        c1 int,
        c2 char(4));
   printf("Second table created...\n");
   EXEC SQL CREATE TABLE qp312_tbl3(
        col1 int,
        col2 char(4));
   printf("Third table created...\n");
   printf("Table creates completed...\n");

   printf("Inserting rows into second table...\n");

   printf("Inserting first row...\n");
   EXEC SQL INSERT INTO qp312_tbl2 values (next value for qp312_intseq,'aaaa');
   printf("Inserting second row...\n");
   EXEC SQL INSERT INTO qp312_tbl2 values (next value for qp312_intseq,'bbbb');
   printf("Inserting third row...\n");
   EXEC SQL INSERT INTO qp312_tbl2 values (current value for qp312_intseq,'cccc');
   printf("Insert completed...\n");

   printf("Updating last row of table...\n");
   EXEC SQL UPDATE qp312_tbl2
        set c1 = next value for qp312_decseq
        where c2 = 'cccc';
   printf("Update completed...\n");

   printf("Now inserting rows into 3rd table with INSERT/SELECT...\n");
   EXEC SQL INSERT into qp312_tbl3
        SELECT next value for qp312_intseq, c2
        FROM qp312_tbl2;
   printf("Insert completed...\n");

   printf("Now verifying data in tables...\n");
   printf("Verifying first table...\n");
   printf("|---------------|\n");
   printf("|CURRCOL|NEXTCOL|\n");
   EXEC SQL SELECT currcol, nextcol INTO :tbl1c1, :tbl1c2 
      FROM qp312_tbl1 order by 1, 2;
   EXEC SQL BEGIN;
      printf("|-------|-------|\n");
      printf("|%7.0f|%7.0f|\n",tbl1c1,tbl1c2);
   EXEC SQL END;
   printf("|---------------|\n");
   printf("Verifying second table...\n");
   printf("|---------|\n");
   printf("|  c1|  c2|\n");
   printf("|----|----|\n");
   EXEC SQL SELECT * INTO :tbl2c1, :tbl2c2 
      FROM qp312_tbl2 order by 2, 1;
   EXEC SQL BEGIN;
      printf("|%4d|%s|\n",tbl2c1,tbl2c2);
   EXEC SQL END;
   printf("|---------|\n");
   printf("Verifying third table...\n");
   printf("|---------|\n");
   printf("|col1|col2|\n");
   printf("|----|----|\n");
   EXEC SQL SELECT * INTO :tbl3c1, :tbl3c2
      FROM qp312_tbl3 order by 2, 1;
   EXEC SQL BEGIN;
      printf("|%4d|%s|\n",tbl3c1,tbl3c2);
   EXEC SQL END;
   printf("|---------|\n");
   printf("Verification completed...\n");

   printf("Operation completed\n");

   EXEC SQL DISCONNECT;
}
