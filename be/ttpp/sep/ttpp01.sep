    update t2 from gtt1 set col2='update' where t2.col1=gtt1.col1 and 
    gtt1.col1=4000;
end; 

create procedure p5( gtt1 set of (col1 int, col2 char(8) )) as
begin
   delete from t2 where t2.col1 in (select gtt1.col1 from gtt1 
   where t2.col1=gtt1.col1 and char(gtt1.col2,8)='gt.new' and gtt1.col1<=3000) ;
   delete from t1 where char(t1.col1,8) in (select col2 from gtt1);
end 

create procedure p6 (gtt1 set of (col1 int, col2 char(8))) as
begin
   delete from t2 where t2.col1 in (select gtt1.col1 from gtt1);
end 

