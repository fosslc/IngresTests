			printf("\t\t\tIIAPI_GQF_STATEMENT\n");
		if (iiapi_getqinfoparm->gq_flags&IIAPI_GQF_TRANSACTION_INACTIVE)
			printf("\t\t\tIIAPI_GQF_TRANSACTION_INACTIVE\n");
